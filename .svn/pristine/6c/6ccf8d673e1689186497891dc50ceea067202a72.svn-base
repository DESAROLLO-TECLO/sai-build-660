<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
<title>Insert title here</title>
</head>
<body>
	<section class="content-header">
		<h1>Remisión sin Infracción</h1>
		<ol class="breadcrumb">
			<li><a><i class="fa fa-safari"></i>Remisión a Depósito</a></li>
			<li class="active">Sin Infracción</li>
		</ol>
	</section>
	
	<section class="content" ng-if="validar.message">
		<div class="row col-md-8 col-md-offset-2">
			<div class="alert alert-teclo  alert-dismissable center">
				<strong class="mensajeAutorizacion">{{validar.message}}</strong>
			</div>
		</div>
	</section>
	
	<section class="content" ng-if="validar.viewFormulario && validar.access">
		<form name="forms.altaInfraccion" role="form" novalidate>
			<div class="row">
				<div class="col-md-6 connectedSortable">
					<div class="box box-danger">
						<div class="box-header with-border text-center">
							<h3 class="box-title">Ingreso de vehículo</h3>
						</div>
						<div class="box-body">
							<div class="row">
								<div class="col-md-6">
									<div class="form-group" ng-class="{'has-error': forms.altaInfraccion.nvehiculo.$invalid && forms.altaInfraccion.nvehiculo.$dirty}">
										<label>No. Serie de Vehículo:</label>
										<div class="input-group">
											<span class="input-group-addon"> <i class="fa fa-pencil" aria-hidden="true"></i>
											</span> <input type="text" name="nvehiculo" id="nvehiculo"
												ng-model="ingresoSinInfracVO.num_serie_vehiculo"
												maxlength="18" class="form-control" capitalize autofocus
												ng-required="false" ng-blur="tab($event,'causa')"></input>
										</div>
										<div
											ng-if="forms.altaInfraccion.nvehiculo.$error.required && forms.altaInfraccion.nvehiculo.$dirty"
											ng-style="{color:'red'}">El no. serie de vehículo es
											requerido</div>
									</div>
								</div>
								
								<div class="col-md-6">
									<label>No. Control Interno:</label>
									<div class="input-group">
										<span class="input-group-addon"> <i
											class="fa fa-pencil" aria-hidden="true"></i>
										</span> <input type="text" class="form-control" ng-disabled="true"></input>
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-6">
									<div class="form-group"
										ng-class="{'has-error': forms.altaInfraccion.resguardo.$invalid && forms.altaInfraccion.resguardo.$dirty}">
										<label>No. de Resguardo:</label>
										<div class="input-group">
											<span class="input-group-addon"> <i
												class="fa fa-pencil" aria-hidden="true"></i>
											</span> <input type="text" name="resguardo"
												ng-model="ingresoSinInfracVO.num_resguardo"
												class="form-control" capitalize ng-disabled="true"></input>
										</div>
										<div
											ng-if="forms.altaInfraccion.resguardo.$error.required && forms.altaInfraccion.resguardo.$dirty"
											ng-style="{color:'red'}">El no. de resguardo es
											requerido</div>
									</div>
								</div>
								
								<div class="col-md-6">
									<div class="form-group"
										ng-class="{'has-error': forms.altaInfraccion.depo.$invalid && forms.altaInfraccion.depo.$dirty}">
										<label>*Depósito:</label>
										<div class="input-group">
											<div class="input-group-addon">
												<i class="fa fa-list-ul"></i>
											</div>
											<select disabled class="form-control" name="depo" id="depo"
												style="width: 100%;"
												ng-options="depo as depo.dep_nombre for depo in listDeposito"
												ng-model="ingresoSinInfracVO.deposito">
											</select>
										</div>
										<div
											ng-if="forms.altaInfraccion.depo.$error.required && forms.altaInfraccion.depo.$dirty"
											ng-style="{color:'red'}">El Depósito es requerido</div>
									</div>
								</div>
							</div>
							<div ng-if="viewOficialData">
								<br><br><br>
							</div>
						</div>
					</div><!-- TERMINA BOX DANGER INGRESO DE VEHÍCULO -->
					<div class="box box-danger">
						<div class="box-header with-border text-center">
							<h3 class="box-title">Datos del Vehículo</h3>
						</div>
						<div class="box-body">
							<div class="row">
								<div class="col-md-6">
									<div class="form-group"
										ng-class="{'has-error': forms.altaInfraccion.si_placa_i.$invalid && forms.altaInfraccion.si_placa_i.$dirty}">
										<label>*Tiene Placa/Permiso:</label>
										<div class="input-group">
											<div class="radio-inline">
												<label><input ng-change="cambioCuentaConPlaca('S')"
													type="radio" ng-model="ingresoSinInfracVO.infrac_con_placa"
													name="si_placa_i" id="si_placa_i" value="S"
													ng-required="!validar.viewFormInfraccion">SI</label>
											</div>
											<div class="radio-inline">
												<label><input ng-change="cambioCuentaConPlaca('N')"
													type="radio" ng-model="ingresoSinInfracVO.infrac_con_placa"
													name="si_placa_i" id="si_placa_i_2" value="N"
													ng-required="!validar.viewFormInfraccion">NO</label>
											</div>
										</div>

										<div
											ng-if="forms.altaInfraccion.si_placa_i.$error.required && forms.altaInfraccion.si_placa_i.$dirty"
											ng-style="{color:'red'}">La valoración tiene
											placa/permiso es requerido</div>
									</div>
								</div>
								<div class="col-md-6">
									<div class="form-group"
										ng-class="{'has-error': forms.altaInfraccion.placaVe.$invalid && forms.altaInfraccion.placaVe.$dirty}">
										<label>*Placa:</label>
										<div class="input-group">
											<div class="input-group-addon">
												<i class="fa fa-car"></i>
											</div>
											<input
												ng-disabled="ingresoSinInfracVO.infrac_con_placa == 'N'"
												ng-required="ingresoSinInfracVO.infrac_con_placa != 'N'"
												type="text" name="placaVe" capitalize maxlength="9"
												ng-model="ingresoSinInfracVO.infrac_placa"
												class="form-control"></input>
										</div>
										<div
											ng-if="forms.altaInfraccion.placaVe.$error.required && forms.altaInfraccion.placaVe.$dirty"
											ng-style="{color:'red'}">La placa es requerida</div>
									</div>
								</div>
								<div class="col-md-6 col-xs-12">
									<div class="form-group"
										ng-class="{'has-error': forms.altaInfraccion.vorigen_i.$invalid && forms.altaInfraccion.vorigen_i.$dirty}">
										<label>Origen:</label>
										<div class="input-group">
											<div class="radio-inline">
												<label><input type="radio" ng-change="cambiarOrigenv('N')"
													ng-model="ingresoSinInfracVO.vorigen" name="vorigen_i"
													id="vorigen_i" value="N">Nacional</label>
											</div>
											<div class="radio-inline">
												<label><input type="radio" ng-change="cambiarOrigenv('E')"
													ng-model="ingresoSinInfracVO.vorigen" name="vorigen_i"
													id="vorigen2_i" value="E">Extranjero</label>
											</div>
										</div>
										<div
											ng-if="forms.altaInfraccion.vorigen_i.$error.required && forms.altaInfraccion.vorigen_i.$dirty"
											ng-style="{color:'red'}">El origen es requerido</div>
									</div>
								</div>
								<div class="col-md-6 col-xs-12">
									<div class="form-group"
										ng-class="{'has-error': forms.altaInfraccion.tipo.$invalid && forms.altaInfraccion.tipo.$dirty}">
										<label>*Tipo:</label>
										<div class="input-group">
											<div class="input-group-addon">
												<i class="fa fa-list-ul"></i>
											</div>

											<select class="form-control" name="tipo" id="tipo"
												style="width: 100%;"
												ng-options="tipo as tipo.vTipoNombre for tipo in listTipoV"
												ng-model="ingresoSinInfracVO.vtipo_cod"
												ng-required="!validar.viewFormInfraccion">
												<option value="" selected="selected">Seleccione</option>
											</select>
										</div>


										<div
											ng-if="forms.altaInfraccion.tipo.$error.required && forms.altaInfraccion.tipo.$dirty"
											ng-style="{color:'red'}">El tipo es requerido</div>
									</div>
								</div>
								<div class="col-md-6 col-xs-12">
									<div class="form-group"
										ng-class="{'has-error': forms.altaInfraccion.marca.$invalid && forms.altaInfraccion.marca.$dirty}">
										<label>Marca:</label>
										<div class="input-group">
											<div class="input-group-addon">
												<i class="fa fa-registered"></i>
											</div>

											<select class="form-control" name="marca" id="origen"
												style="width: 100%;"
												ng-options="marca as marca.vMarNombre for marca in listMarca"
												ng-model="ingresoSinInfracVO.vmar_cod"
												ng-change="listModel()">
												<option value="" selected="selected">Seleccione</option>
											</select>
										</div>


										<div
											ng-if="forms.altaInfraccion.marca.$error.required && forms.altaInfraccion.marca.$dirty"
											ng-style="{color:'red'}">La marca es requerida</div>
									</div>
								</div>
								<div class="col-md-6 col-xs-12">
									<div class="form-group"
										ng-class="{'has-error': forms.altaInfraccion.modelo.$invalid && forms.altaInfraccion.modelo.$dirty}">
										<label>Modelo:</label>
										<div class="input-group">
											<div class="input-group-addon">
												<i class="fa fa-barcode"></i>
											</div>
											<select ng-disabled="!ingresoSinInfracVO.vmar_cod"
												class="form-control" name="modelo" id="modelo"
												style="width: 100%;"
												ng-options="modelo as modelo.vModNombre for modelo in listModelo"
												ng-model="ingresoSinInfracVO.vmod_cod">
												<option value="" selected="selected">Seleccione</option>
											</select>
										</div>

										<div
											ng-show="forms.altaInfraccion.modelo.$error.required && forms.altaInfraccion.modelo.$dirty"
											ng-style="{color:'red'}">El modelo es requerido</div>
									</div>
								</div>
								<div class="col-md-6 col-xs-12">
									<div class="form-group"
										ng-class="{'has-error': forms.altaInfraccion.color.$invalid && forms.altaInfraccion.color.$dirty}">
										<label>Color:</label>
										<div class="input-group">
											<div class="input-group-addon">
												<i class="fa fa-paint-brush"></i>
											</div>

											<select class="form-control" name="color" id="color"
												style="width: 100%;" ng-blur="tab($event, firstCheck)"
												ng-options="color as color.vColorNombre for color in listColor"
												ng-model="ingresoSinInfracVO.vcolor_cod">
												<option value="" selected="selected">Seleccione</option>
											</select>
										</div>

										<div
											ng-show="forms.altaInfraccion.color.$error.required && forms.altaInfraccion.color.$dirty"
											ng-style="{color:'red'}">El color es requerido</div>
									</div>
								</div>
							</div>
							<div ng-if="viewOficialData">
								<br><br>
							</div>
						</div>
					</div><!-- TERMINA BOX DANGER DATOS DEL VEHÍCULO -->
				</div>	
				<div class="col-md-6 connectedSortable">
					<div class="box box-danger">
						<div class="box-header with-border text-center">
							<h3 class="box-title">Datos del Ingreso</h3>
						</div>
						<div class="box-body">
							<div class="row">
								<div class="col-md-6">
									<div class="form-group"
										ng-class="{'has-error': forms.altaInfraccion.causa.$invalid && forms.altaInfraccion.causa.$dirty}">
										<label>*Causa de Ingreso:</label>
										<div class="input-group">
											<div class="input-group-addon">
												<i class="fa fa-list-ul"></i>
											</div>
											<select class="form-control" name="causa" id="causa"
												style="width: 100%;"
												ng-options="causa.causa_cod as causa.causa_nombre for causa in listCausa"
												ng-model="ingresoSinInfracVO.causa_id"
												ng-change="verificaIngreso()" ng-required="true">
												<option value="" selected="selected">Seleccione</option>
											</select>
										</div>

										<div
											ng-show="forms.altaInfraccion.causa.$error.required && forms.altaInfraccion.causa.$dirty"
											ng-style="{color:'red'}">La causa ingreso es requerida</div>
									</div>
								</div>
								<div class="col-md-6">
									<div class="form-group"
										ng-class="{'has-error': forms.altaInfraccion.operativo.$invalid && forms.altaInfraccion.operativo.$dirty}">
										<label>Operativo:</label>
										<div class="input-group">
											<div class="input-group-addon">
												<i class="fa fa-list-ul"></i>
											</div>
											<select class="form-control" name="operativo" id="operativo"
												style="width: 100%;"
												ng-options="oper.oper_cod as oper.oper_nombre for oper in listOperativo"
												ng-model="ingresoSinInfracVO.operativo" ng-required="false">
											</select>
										</div>
										<div
											ng-if="forms.altaInfraccion.operativo.$error.required && forms.altaInfraccion.operativo.$dirty"
											ng-style="{color:'red'}">El operativo es requerido</div>
									</div>
								</div>
								<div class="col-md-12">
								</div>
								<div class="col-md-6">
									<div class="form-group"
										ng-class="{'has-error': forms.altaInfraccion.docu.$invalid && forms.altaInfraccion.docu.$dirty}">
										<label>*Documento:</label>
										<div class="input-group">
											<div class="input-group-addon">
												<i class="fa fa-file-text-o"></i>
											</div>
											<input ng-disabled="validar.disabledDocumento" type="text"
												name="docu" maxlength=15
												ng-model="ingresoSinInfracVO.documento" class="form-control"
												capitalize ng-required="!validar.disabledDocumento"></input>
										</div>
										<div
											ng-if="forms.altaInfraccion.docu.$error.required && forms.altaInfraccion.docu.$dirty"
											ng-style="{color:'red'}">El documento es requerido</div>
									</div>
								</div>
								<div class="col-md-6">
									<div class="form-group"
										ng-class="{'has-error': forms.altaInfraccion.folio.$invalid && forms.altaInfraccion.folio.$dirty}">
										<label>*Infracción Impresa:</label>
										<div class="input-group">
											<div class="input-group-addon">
												<i class="fa fa-pencil"></i>
											</div>
											<input type="text" name="folio"
												ng-model="ingresoSinInfracVO.DG.infrac_impresa" class="form-control"
												capitalize maxlength=18
												ng-required="validar.viewFormInfraccion"
												ng-disabled="validar.disabledFImpresa"></input>
										</div>
										<div
											ng-if="forms.altaInfraccion.folio.$error.required && forms.altaInfraccion.folio.$dirty"
											ng-style="{color:'red'}">El folio de boleta impresa es
											requerido</div>
									</div>
								</div>
								<div class="col-md-12">
								</div>
								<div class="col-md-6">
									<div class="form-group"
										ng-class="{'has-error': forms.altaInfraccion.medio.$invalid && forms.altaInfraccion.medio.$dirty}">
										<label>*Medio de Transporte Entrada:</label>
										<div class="input-group">
											<div class="input-group-addon">
												<i class="fa fa-list-ul"></i>
											</div>
											<select class="form-control" name="medio" id="medio"
												style="width: 100%;"
												ng-options="medioTrans.t_ingr_cod as medioTrans.t_ingr_nombre for medioTrans in listMedios"
												ng-model="ingresoSinInfracVO.t_ingr_id" ng-required="true"
												ng-change="cambioCuentaConGrua()">
												<option value="" selected="selected">Seleccione</option>
											</select>
										</div>

										<div
											ng-if="forms.altaInfraccion.medio.$error.required && forms.altaInfraccion.medio.$dirty"
											ng-style="{color:'red'}">El medio de transporte entrada
											es requerido</div>
									</div>
								</div>
								<div class="col-md-6">
									<div class="form-group"
										ng-class="{'has-error': forms.altaInfraccion.econo.$invalid && forms.altaInfraccion.econo.$dirty}">
										<label>*No. Económico:</label>
										<div class="input-group">
											<div class="input-group-addon">
												<i class="fa fa-pencil"></i>
											</div>
											<input
												ng-disabled="ingresoSinInfracVO.t_ingr_id == '01' && ingresoSinInfracVO.grua_id != 168"
												type="text" name="econo"
												ng-model="ingresoSinInfracVO.grua_cod"
												ng-required="ingresoSinInfracVO.t_ingr_id != '01' || ingresoSinInfracVO.grua_id == 168"
												class="form-control" maxlength=15></input>
										</div>
										<div
											ng-if="forms.altaInfraccion.econo.$error.required && forms.altaInfraccion.econo.$dirty"
											ng-style="{color:'red'}">El no. económico es requerido</div>
									</div>
								</div>
								<div class="row">
									<div class="col-md-12">
									</div>
								</div>
								<div class="col-md-6" >
									<div class="form-group"
										ng-class="{'has-error': forms.altaInfraccion.nogrua.$invalid && forms.altaInfraccion.nogrua.$dirty}">
										<label>*No. de Grúa:</label>
										<div class="input-group">
											<div class="input-group-addon">
												<i class="fa fa-list-ul"></i>
											</div>
											<select class="form-control" name="nogrua" id="nogrua"
												style="width: 100%;" ng-disabled="ingresoSinInfracVO.t_ingr_id != '01'"
												ng-options="gruas.grua_cod as gruas.grua_cod for gruas in listaGruas"
												ng-model="ingresoSinInfracVO.grua_cod_combo"
												ng-required="ingresoSinInfracVO.t_ingr_id == '01' ">
												<option value="" selected="selected">Seleccione</option>
											</select>
										</div>

										<div
											ng-if="forms.altaInfraccion.nogrua.$error.required && forms.altaInfraccion.nogrua.$dirty"
											ng-style="{color:'red'}">El no. de grúa es requerido</div>
									</div>
								</div>
								<div class="col-md-6">
									<div class="form-group"
										ng-class="{'has-error': forms.altaInfraccion.origen.$invalid && forms.altaInfraccion.origen.$dirty}">
										<label>Depósito Origen:</label>
										<div class="input-group">
											<div class="input-group-addon">
												<i class="fa fa-list-ul"></i>
											</div>
											<select disabled class="form-control" name="medio" id="medio"
												style="width: 100%;">
												<option value="" selected="selected">Seleccione</option>
											</select>
										</div>
									</div>
									<br>
								</div>
								<div class="col-md-6">
									<div class="form-group"
										ng-class="{'has-error': forms.altaInfraccion.infra_ley.$invalid && forms.altaInfraccion.infra_ley.$dirty}">
										<label>*Es una Violación a la Ley del Transporte:</label>
										<div class="input-group">
											<div class="radio-inline">
												<label><input type="radio"
													ng-model="FI.violacionLeyTransporte"
													name="infra_ley" id="infra_ley" value="S" ng-required="true" >SÍ</label>
											</div>
											<div class="radio-inline">
												<label><input type="radio"
													ng-model="FI.violacionLeyTransporte"
													name="infra_ley" id="infra_ley2" value="N" ng-required="true" >NO</label>
											</div>
										</div>
										<div
											ng-if="forms.altaInfraccion.infra_ley.$error.required && forms.altaInfraccion.infra_ley.$dirty"
											ng-style="{color:'red'}">La valoración a la ley de
											trasporte es requerida</div>
									</div>
								</div>
								<div class="col-md-6">
									<div class="form-group"
										ng-class="{'has-error': forms.altaInfraccion.v_sellado.$invalid && forms.altaInfraccion.v_sellado.$dirty}">
										<label>*Vehículo Sellado en:</label>
										<div class="input-group">
											<div class="radio-inline">
												<label><input type="radio"
													ng-model="ingresoSinInfracVO.v_sellado" name="v_sellado"
													id="v_sellado" value="D" ng-required="true">Depósito</label>
											</div>
											<div class="radio-inline">
												<label><input type="radio"
													ng-model="ingresoSinInfracVO.v_sellado" name="v_sellado"
													id="v_sellado2" value="A" ng-required="true">Arrastre</label>
											</div>
										</div>
										<div
											ng-if="forms.altaInfraccion.v_sellado.$error.required && forms.altaInfraccion.v_sellado.$dirty"
											ng-style="{color:'red'}">La valoración vehículo sellado
											es requerido</div>
									</div>
								</div>
								<div class="col-md-6">
									<div class="form-group"
										ng-class="{'has-error': forms.altaInfraccion.v_cajuela.$invalid && forms.altaInfraccion.v_cajuela.$dirty}">
										<label>Cajuela Abierta:</label>
