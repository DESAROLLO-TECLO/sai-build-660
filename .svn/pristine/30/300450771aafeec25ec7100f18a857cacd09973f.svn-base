<html>

<head>
<meta charset="utf-8">
<title>Atención Ciudadana</title>
</head>

<body>
      <section class="content-header">
		<h1>Consulta de Trámite</h1>
		<ol class="breadcrumb">
			<li><a><i class="fa fa-book"></i>Atención Ciudadana</a></li>
			<li class="active">Consulta</li>
		</ol>
	</section>

	
	<section class="content">
		<form id="form1" role="form" name="detalleTramite">
			<div class="row">
				<section class="col-lg-12 connectedSortable">
					<div class="box box-danger">
						<div class="box-header with-border text-center">
							<h3 class="box-title">Datos Generales</h3>
						</div>
						<div class="box-body">
							<div class="form-group">
	                                <div class="row">
	                                
	                               	<div class="col-sm-4">
									<div class="form-group">

										<label for="example1">Fecha de Trámite:</label>
										<div class="input-group date" datetimepicker
											options="dateTimePickerOptions"
											name="DGFecha" ng-model="DG.fecha" on-hide="updateFIFecha()"
											show-button-bar="true" >

											<div class="input-group-addon">
												<i class="fa fa-calendar"></i>
											</div>
											<input type="text" ng-disabled="true"
												class="form-control inputFecha input-append"
												name="txtDGFecha" readonly />
										</div>
									</div>
								</div>
								
									
									<div class="col-md-4">
										<div class="form-group">
											<label for="tipoTramite">Folio de Trámite:</label>
											<div class="input-group">
												<div class="input-group-addon">
													<i class="fa fa-list-alt"></i>
												</div>
												<input type="text" class="form-control" capitalize
													disabled="true" id="folioTramite" ng-model="tramiteVO.idACTramite"
													ng-value="tramiteVO.idACTramite">
											</div>
										</div>
									</div>
									
									
									
									<div class="col-md-4">
									<div class="form-group">
										<label for="tipoTramite">Tipo de Trámite:</label>
										<div class="input-group">
											<div class="input-group-addon">
												<i class="fa fa-list-alt"></i>
											</div>
											<select class="form-control" select-picker multiple
												 name="tipoTramite" id="tipoTramite"
												data-actions-box="true"
												data-selected-text-format="count > 1" data-max-options="6"
												ng-model="DG.tipoTramite"
												ng-options="tt.idTramite as tt.nbTramite for tt in comboTipoTramite" ng-change="valoresPorDefectoTramite()">
											</select>


										</div>
									</div>
								</div>
                                 </div>
                                 
                                 
                                 
                            <div class="row">
                            	<div class="col-sm-4">
									<div class="form-group">
										<label>Placa Oficial:</label>
										<div class="input-group">
											<div class="input-group-addon">
												<i class="fa fa-shield"></i>
											</div>
											<input type="text" class="form-control" disabled="true"
												ng-model="DG.empleadoPlaca"
												name="DGEmpleado" >
												<span class="input-group-btn">
													<button ng-disabled="true" type ="button" class="btn btn-danger btn-flat"
														ng-click="">Buscar
													</button>
												</span>
										</div>
										<div>{{DG.nombre}}</div>
									</div>
								</div>
                            
                            <div class="col-sm-4">
									<div class="form-group" >
										<label for="medioSolicitud">Origen de Solicitud:</label>
										<div class="input-group">
											<span class="input-group-addon"><i class="fa fa-list-alt"></i></span>
											<select class="form-control"
												 name="medioSolicitud" id="medioSolicitud"
												ng-model="DG.medioSolicitud"
												ng-change="valoresPorDefectoSolicitud()"
												ng-options="cs.idMedioSolicitud as cs.nbMedioSolicitud for cs in comboOrigenSolicitud">
											</select>
										</div>
									</div>
								</div>
                            
                            
			                  	
								<div class="col-sm-4 center">
								<label fo="checkTipoPersona">¿Solicitante?</label>
									<div class="form-grouo">
										<div class="row centerRow">
												<switcher class="swTeclo pull-right" ng-disabled="true"
												id-check="checkTipoPersona"
												true-label="Empresa"
												true-value="true"
												false-label="Ciudadano"
												false-value="false"
												ng-model="tipoPersona">
											
											</switcher>
										</div>
									</div>
								</div>
							</div>
							 <div class="row col-md-offset-1">
							 	<span><strong> Trámites Seleccionados:</strong> {{DG.nbTramite+""}} </span>
							 </div>
							
						</div>
						</div>
					</div>
				</section>
		





			
	
			
		<section class="col-lg-6 connectedSortable">
					<div class="box box-danger">
						<div class="box-header with-border text-center">
							<h3 class="box-title" ng-if="!tipoPersona">Datos del Ciudadano</h3>
							<h3 class="box-title" ng-if="tipoPersona">Datos del Representante</h3>
						</div>
						<div class="box-body">
							<div class="row">
							
								<div class="col-sm-6">
									<div class="form-group">
										<label for="DCAPaterno">Apellido Paterno:</label>
										<div class="input-group">
											<span class="input-group-addon"><i class="fa fa-user"></i></span>
											<input ng-disabled="true" type="text"
												class="form-control" capitalize id="apellidoPaterno"
												name="DVApellidoP" ng-model="DC.apellidoPaterno"
												maxlength="50">
										</div>
									</div>
								</div>
							
							
							<div class="col-sm-6">
									<div class="form-group">
										<label for="DCAMatenro">Apellido Materno:</label>
										<div class="input-group">
											<span class="input-group-addon"><i class="fa fa-user"></i></span>
											<input ng-disabled="true" type="text"
												class="form-control " capitalize capitalize
												id="apellidoMaterno" name="DVApellidoM"
												ng-model="DC.apellidoMaterno" maxlength="50">
										</div>

									</div>
								</div>
	                              
	                              
	                               <div class="col-sm-6">

									<div class="form-group">
										
										<label for="DCNombre">Nombre:</label>
										<div class="input-group">
											<span class="input-group-addon"><i
												class="fa fa-user-plus"></i></span> <input type="text"
												class="form-control " capitalize disabled="true"
												id="DCNombre" ng-model="DC.nombre" name="DVNombre"
												maxlength="50" />
										</div>
									</div>
								</div>
								
									<div class="col-sm-6">
									<div class="form-group">
										<label for="tel">Teléfono:</label>
										<div class="input-group">
											<span class="input-group-addon"><i class="fa fa-phone"></i></span>
											<input type="text" class="form-control" id="tel"
												ng-model="DC.numTelefono" name="DVTelefono"
												ng-disabled="true" mask="(999) 999-9999">
										</div>
									</div>
								</div>
							</div>
								
							<div class="row">
							
							<div class="col-sm-6">
									<div class="form-group">
									
									<label for="DCEmpresa">Empresa:</label>
										<div class="input-group">
											<span class="input-group-addon"><i
												class="fa fa-user-plus"></i></span> <input type="text"
												class="form-control " capitalize capitalize id="emp"
												ng-model="DC.emp" maxlength="40" disabled="true" name="empresa">
										</div>
									</div>
								</div>
								<div class="col-sm-6">
									<div class="form-group">
										<label for="mail">Correo:</label>
										<div class="input-group">
											<span class="input-group-addon"><i class="fa fa-at"></i></span>
											<input type="email" class="form-control " capitalize
												capitalize id="mail" ng-model="DC.correo" maxlength="40"
												name="DVCorreo" ng-disabled="true">
										</div>
									</div>
								</div>
							</div>
							
							<br/>
							<h4 class="box-title">Dirección</h4>
							<hr/>
							

							<div class="row">
										<div class="col-sm-6">
									<div class="form-group">
										<label for="DCcalle">Calle: </label>
										<div class="input-group">
											<span class="input-group-addon"><i
												class="fa fa-long-arrow-up"></i></span> <input type="text"
												class="form-control " capitalize  id="DCcalle"
												ng-model="DC.calle" maxlength="45"
												ng-disabled="true" name="calle">
										</div>
									</div>
								</div>
								<div class="col-sm-6">
									<div class="form-group">
										<label for="DCcolonia">Colonia:</label>
										<div class="input-group">
											<span class="input-group-addon"><i
												class="fa fa-map-marker"></i></span> <input type="text"
												class="form-control " capitalize capitalize id="DCcolonia"
												ng-model="DC.colonia" maxlength="45"
												ng-disabled="true" name="colonia">
										</div>
									</div>
								</div>
						



				
								<div class="col-sm-6">
									<div class="form-group">
										<label for="DCnumExt">No. Ext: </label>
										<div class="input-group">
											<span class="input-group-addon"><i
												class="fa fa-sort-numeric-asc"></i></span> <input type="text"
												class="form-control " capitalize id="DCnumExt"
												ng-disabled="true" maxlength="10" ng-model="DC.numeroExterior">
										</div>
									</div>
								</div>
								<div class="col-sm-6">
									<div class="form-group">
										<label for="DCnumInt">No. Int: </label>
										<div class="input-group">
											<span class="input-group-addon"><i
												class="fa fa-sort-numeric-asc"></i></span> <input type="text"
												class="form-control" capitalize id="DCnumInt"
											     ng-disabled="true" maxlength="10" ng-model="DC.numeroInterior">
										</div>
									</div>
								</div>
						


							
								<div class="col-sm-6">
									<div class="form-group">
										<label>Estado: </label>
										<div class="input-group">
											<span class="input-group-addon"><i class="fa fa-map"></i></span>
											<select name="DCEstado" class="form-control"
												ng-options="estado.edoId as estado.edoNombre for estado in catalogoEstados | orderBy:'edoNombre'"
												ng-model="DC.direccionEstado"
												 ng-disabled="true" name="estado">
												 <option value="">Seleccione</option>
											</select>
										</div>
									</div>
								</div>
								<div class="col-sm-6">
									<div class="form-group">
										<label>Delegación / Municipio:</label>
										<div class="input-group">
											<span class="input-group-addon"><i
												class="fa fa-map-marker"></i></span> <select name="DCMun"
												class="form-control" ng-disabled="true"
												ng-options="del.delId.delId as del.delNombre for del in catalogoDelegaciones | orderBy:'delNombre'"
												ng-model="DC.delegacion">
												<option value="">Seleccione</option>
											</select>
										</div>
									</div>
								</div>
								
						
							
							</div>
						</div>
					</div>
				</section> 
				
				
					<section class="col-lg-6 connectedSortable">
					<div class="box box-danger">
						<div class="box-header with-border text-center">
							<h3 class="box-title">Datos del Vehículo</h3>
						</div>


						<div class="box-body">
							<div class="row">
							<div class="col-sm-6">
									<div class="form-group">
										<label for="DVPlaca">Placa Vehículo:</label>
										<div class="input-group">
											<span class="input-group-addon"><i
												class="fa fa-automobile"></i></span> <input type="text"
												 class="form-control "
												capitalize id="DVPlaca" ng-model="DV.placa"
												ng-disabled="true" name="DVPlaca" maxlength="9" />
										</div>
									</div>
								</div>
								
						             <div class="col-sm-6">
									<div class="form-group">
										<label>Tipo:</label>
										<div class="input-group">
											<span class="input-group-addon"><i
												class="fa fa-ellipsis-v "></i></span> <select class="form-control"
												ng-options="tipoV.vTipoId as tipoV.vTipoNombre for tipoV in catalogoTiposVehiculo | orderBy:'vTipoNombre'"
												ng-model="DV.tipoVehiculo" name="DVTipoVehiculo"
												ng-disabled="true">
												<option value="" selected="selected">Seleccione</option>
											</select>
										</div>

									</div>
								</div>
								</div>
								
								<div class="row">
									<div class="col-sm-6">
									<label>Marca: </label>
									<div class="input-group">
										<span class="input-group-addon"><i
											class="fa fa-registered"></i></span> <select name="vMarca"
											class="form-control"
											ng-options="marca.vMarId as marca.vMarNombre for marca in catalogoVehiculosMarcas"
											ng-model="DV.vehiculoMarca"
											ng-disabled="true">
											<option value="" selected="selected">Seleccione</option>
										</select>
									</div>

								</div>
							
								
									<div class="col-sm-6">
									<div class="form-group">
									
										<label for="DVMarcaOtro">Marca Otro:</label>
										<div class="input-group">
											<span class="input-group-addon"><i class="fa fa-registered"></i></span>
												<input type="text"  ng-disabled="true"
													class="form-control " capitalize id="DVMarcaOtro" ng-model="DV.MarcaOtro"
													name="DVMarcaOtro" maxlength="50" />
										</div>
									</div>
								</div>
								</div>
								
								<div class="row">
							    
								<div class="col-sm-6">
									<label>Modelo: </label>
									<div class="input-group">
										<span class="input-group-addon"><i
											class="fa fa-barcode"></i></span> <select name="vModelo"
											class="form-control" ng-disabled="true"
											ng-options="modelo.vModId.vModId as modelo.vModNombre for modelo in catalogoVehiculosModelo"
											ng-model="DV.vehiculoModelo">
											<option value="">Seleccione</option>
										</select>
									</div>
					
								</div>
								
                              		<div class="col-sm-6">
									<div class="form-group">
										<label for="DVModeloOtro" >Modelo Otro:</label>
										<div class="input-group">
											<span class="input-group-addon"><i class="fa fa-registered"></i></span>
												<input type="text"  ng-disabled="true"
													class="form-control " capitalize id="DVModeloOtro" ng-model="DV.ModeloOtro"
													name="DVModeloOtro" maxlength="50" />
										</div>
									</div>
								</div>
								</div>
								
								    
								    <div class="row">
									
									
								<div class="col-sm-6">
									<label>Color: </label>
									<div class="input-group">
										<span class="input-group-addon"><i
											class="fa fa-paint-brush"></i></span> <select name="vColor"
											class="form-control"
											ng-options="color.vColorId as color.vColorNombre for color in catalogoVehiculosColor"
											ng-model="DV.vehiculoColor" ng-disabled="true">
											<option value="">Seleccione</option>
										</select>
									</div>

								</div>
								<div class="col-sm-6">
									<div class="form-group">
										<label for="DVcc">Con Copia A:</label>
										<div class="input-group">
											<span class="input-group-addon"><i
												class="fa fa-user-plus"></i></span> 
												
												<input type="text" class="form-control text-lowercase" 
													id="DVcc" ng-model="DV.cc" maxlength="200"
													rows="9" name="DVcc" 
													placeholder="C.C." ng-disabled="true">
													<span class="input-group-btn">
													<button type ="button" class="btn btn-danger btn-flat"
														ng-click="validarcc()" ng-disabled="true">Agregar
													</button>
												</span>
										</div>
                                      <span class="col-md-offset-5" data-toggle="tooltip" data-placement="top"
									data-html="true"
									data-original-title="{{tituloSug}}
         					{{descripcion}}
         					<!--{{descripcion2}}-->
         					{{formato}}
         					{{tituloEjemplo}}
         					{{ejemplo}}">
									<i class="fa fa-question-circle"></i> Ayuda
								</span>
									</div>
								</div>	
								</div>
								
                                <div class="row">
                                
                                 <div class="col-sm-7">
									<div class="form-group">
										<label for="DVcc">Correos Agregados:</label>			
										<div class="input-group">
											<span class="input-group-addon"><i
												class="fa fa-user-plus"></i></span> 
												<div  class="divTextArea" >
											    <div  ng-class="clasesCss" id="correos1"> </div>
								                <br id="br1">
												</div>
                                        </div>	
									</div>
								</div>	
                                
								</div>
						</div>
						</div>
				</section>
				
				<section class="col-lg-6 connectedSortable">
					<div class="box box-danger">
						<div class="box-header with-border text-center">
							<h3 class="box-title">Documentos</h3>
						</div>
						<div class="box-body">
							<div class="row">
								<div class="col-md-5">
									<div class="row">
									
									<div class="col-sm-12">
									<div class="form-group">
										<label for="tipoDocumento">Documentos a Entregar:</label>
										<div class="input-group">
											<div class="input-group-addon">
												<i class="fa fa-list-alt"></i>
											</div>
											<select class="form-control" select-picker multiple
											 name="tipoDocumento" id="tipoDocumento"
												data-actions-box="true"
												data-selected-text-format="count > 1" data-max-options="8"
												ng-model="DD.tipoDocumento"
												ng-options="td.idDocumento as td.nbDocumento for td in comboTipoDocumento" ng-change="valoresPorDefectoDocumento()">

											</select>
										</div>
									</div>
								</div>
									
									  
									  
									
								      <div class="col-sm-12" > 
										<div class="form-group">
											<label for="DVdocotro">Documento Otro:</label>
											<div class="input-group">
												<span class="input-group-addon"><i
													class="fa fa-user-plus"></i></span> <input type="text"
													ng-disabled="true" class="form-control" capitalize
													id="DVdotro" ng-model="DD.docOtro" name="DVDocOtro"
													maxlength="50" />
											</div>
										</div>
									</div>
				               	</div>
								</div>
								
								
								
								
								<div class="col-sm-7">
								<div class="row">
								   <div class="col-sm-12">
									<div class="form-group">
										<label for="hechos">Hechos: </label>
										<div class="input-group">
											<span class="input-group-addon"><i
												class="fa fa-user-plus"></i></span>
											<textarea type="text" class="form-control " style="resize: none;"
												capitalize id="hechos" ng-model="DD.hechos" maxlength="300"
												rows="4" name="hechos" 
												placeholder="Descripción" ng-disabled="true">
											</textarea>
										</div>
										</div>
									</div>
									
									  <div class="col-sm-12">
									<div class="form-group">
										<label for="DTMCambio">Motivo de Cambio: </label>
										<div class="input-group">
											<span class="input-group-addon"><i class="fa fa-user-plus"></i></span>
											<textarea type="text" class="form-control " capitalize style="resize: none;"
												capitalize id="hechos" ng-model="DD.DTMCambio" maxlength="180"
												rows="4" name="DTMCambio" id="DD.DTMCambio"  disabled="true">
											</textarea>
										</div>
									</div>
								</div>
									
									</div>
								</div>
								
								
								
								
							</div>
							 <div class="row col-md-offset-1">
							 <span ><strong> Documentos a Entregar:</strong> {{DD.nbDocumento+" "+DD.docOtro}} </span>
							 </div>
						</div>
					</div>
				</section>
				
				<section class="col-lg-6 connectedSortable" >
					<div class="box box-danger">
						<div class="box-header with-border text-center">
							<h3 class="box-title">Cambio Tipo de Persona</h3>
						</div>
						<div class="box-body">
							<div class="row">
							  <div class="col-md-6">
									<div class="col-sm-12">
									<div class="form-group" >
										  <label for="infracciones">Folio de Infracción:</label>

										<div class="input-group">
											<span class="input-group-addon"><i
												class="fa fa-user-plus"></i></span> 
												<input type="input" class="form-control text-lowercase" ng-disabled="true"
													 id="infracciones" ng-model="infracciones"  name="infracciones" maxlength="11">
													<span class="input-group-btn">
													<button type ="button" class="btn btn-danger btn-flat"
														ng-click="validarFolios()" ng-disabled="true">Agregar
													</button>
												</span>
											
                                        </div>
								</div>
								</div>
					
						
						     <div class="col-sm-12">
									<div class="form-group">
										<label for="infracAgregadas">Infracciones Agregadas:</label>			
										<div  style="overflow-y: scroll; min-height: 150px; height: 150px;">
											<span class="input-group-addon"><i
												class="fa fa-user-plus"></i></span> 
												<div  class="divTextArea"  >
											    <div  ng-class="clasesCss2" id="infrac1"> </div>
								                <br id="bi1">
												</div>
