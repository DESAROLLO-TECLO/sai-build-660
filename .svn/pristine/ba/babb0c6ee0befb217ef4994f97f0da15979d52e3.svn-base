<html>
<head>
<meta charset="utf-8">
<title>Consulta de Expediente</title>
</head>
<body>
	<div ng-show="error" class="alert alert-warning">
		{{error.message}}</div>
	<section class="content-header">
		<h1>Consulta de Expediente de Infracción</h1>
		<ol class="breadcrumb">
			<li><a href="#"><i class="fa fa-book"></i>Infracción</a></li>
			<li>Consulta</li>
			<li class="active">Expediente</li>
		</ol>
	</section>

	<!-- Main content -->
	<section class="content">
		<div class="row">

			<section class="col-lg-offset-4 col-lg-6 connectedSortable">
				<div class="box box-danger">
					<div class="box-header with-border">
						<h3 class="box-title">Datos Generales</h3>
						<div class="box-tools pull-right">
							<button type="button" class="btn btn-box-tool"
								data-widget="collapse">
								<i class="fa fa-minus"></i>
							</button>
						</div>
					</div>
					<div class="box-body">
						<div class="row">
							<div class="col-xs-12">
								<div class="form-group">
									<img ng-src="data:image/JPEG;base64,{{foto}}">
									<!-- <img id="ItemPreview" src="data:image/png;base64,{{foto}}" />  -->
									<div class="table-responsive">
										<table class="table">
											
										</table>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</section>
		</div>

		<div class="row"></div>
		<!-- {{infraccion}}  -->
		{{$scope.foto}}
	</section>
	<!-- /.content-wrapper -->
	<footer class="main-footer"> </footer>


	<div class="control-sidebar-bg"></div>

	<style type="text/css">
legend.scheduler-border {
	font-size: 1em !important;
	font-weight: bold !important;
	text-align: left !important;
	width: auto;
	padding: 0 10px;
	border-bottom: none;
}

fieldset.scheduler-border {
	border-radius: 0.5em; /* applied border radius */
	border: 1px solid #ccc !important;
	padding: 0 1.4em 1.4em 1.4em !important;
	margin: 0 0 1.5em 0 !important;
}
</style>

</body>
</html>
