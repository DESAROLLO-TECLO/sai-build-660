package mx.com.teclo.saicdmx.persistencia.vo.infracciones;

import java.io.Serializable;

public class TipoArrastreVO implements Serializable
{
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private String	nombreTipoArrastre;
	private String	valorTipoArrastre;
	
	public String getNombreTipoArrastre() 
	{
		return nombreTipoArrastre;
	}

	public String getValorTipoArrastre() 
	{
		return valorTipoArrastre;
	}

	public void setValorTipoArrastre(String valorTipoArrastre) 
	{
		this.valorTipoArrastre = valorTipoArrastre;
	}

	public void setNombreTipoArrastre(String nombreTipoArrastre) 
	{
		this.nombreTipoArrastre = nombreTipoArrastre;
	}
	
	@Override
	public String toString()
	{
		StringBuilder tipoArrastre = new StringBuilder("");
		
		tipoArrastre.append("\n\t nombreTipoArrastre: "	+ nombreTipoArrastre);
		tipoArrastre.append("\n\t valorTipoArrastre: "	+ valorTipoArrastre);
		
		
		return tipoArrastre.toString();
	}	
}
