<html>

<head>
    <meta charset="utf-8">
    <title>Alta de Infracción</title>
</head>

<body>
    <div ng-show="error" class="alert alert-warning">
        {{error.message}}</div>
    <section class="content-header">
        <h1>Alta de Infracción</h1>
        <ol class="breadcrumb">
            <li><a><i class="fa fa-book"></i>Infracción</a></li>
            <li class="active">Alta</li>
        </ol>
    </section>

    <!-- Main content -->
    <section class="content">
        <form id="form1" role="form" name="altaInfraccion">
            <div class="row">
                <section class="col-lg-6 connectedSortable">
                    <div class="box box-danger">
                        <div class="box-header with-border text-center">
                            <h3 class="box-title">Datos Generales</h3>
                        </div>
                        <div class="box-body">
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group" ng-class="{'has-error': altaInfraccion.DGFecha.$invalid && altaInfraccion.DGFecha.$dirty}">

                                            <label for="example1">*Fecha de Infracción:</label>
                                            <div class="input-group date" datetimepicker options="dateTimePickerOptions" ng-required="true"  name="DGFecha" ng-model="DG.fecha" on-hide="updateFIFecha()" show-button-bar="true">

                                                <div class="input-group-addon">
                                                    <i class="fa fa-calendar"></i>
                                                </div>
                                                <input type="text" class="form-control inputFecha input-append"  name = "txtDGFecha" readonly />
                                            </div>

                                            <div ng-show="altaInfraccion.DGFecha.$error.required && altaInfraccion.DGFecha.$dirty" ng-style="{color:'red'}">La fecha de infracción es requerida
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label for="example2">Infracción Impresa:</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i
													class="fa fa-print"></i></span>
                                                <input type="text" integer-number class="form-control" id="example2" ng-model="DG.infrac_impresa" name="DGInfracEmpresa" maxlength="18">
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">

                                    <div class="col-sm-6">

                                        <div class="form-group">
                                            <!-- <span class="badge" style="background-color:transparent; width:50%; padding:0px;"><img src="static/dist/img/loading.gif"  class="ajax-loader img-responsive center-block" style="width:25%; border:0px; margin:0px;"/></span> -->
                                            <label>*Estado:</label>
                                            <br>

                                            <div class="input-group">

                                                <span class="input-group-addon"><i
													class="fa fa-map-marker"></i></span>
                                                <select class="form-control" ng-model="DG.estado" ng-options="edo as edo.edoNombre for edo in DG.estadosDF">
                                                    <!-- disabled="true" 
													<option value="{{DG.estado}}" selected="selected">{{DG.estado.edoNombre}}</option>-->
                                                    <option value="" selected="selected">Seleccione</option>
                                                </select>

                                            </div>
                                        </div>
                                        <!-- /.form-group -->
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group" ng-class="{'has-error': altaInfraccion.DGSector.$invalid && altaInfraccion.DGSector.$dirty}">
                                            <label>*Sector: </label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i
													class="fa fa-pie-chart"></i></span>
                                                <select class="form-control" ng-required="true" name="DGSector" ng-options="sector as sector.secCod + '&nbsp;&nbsp;&nbsp;' + sector.secNombre for sector in DG.sectores" ng-model="DG.Sector" ng-change="actualizarUnidadTerritorialPorSector()">
                                                    <option value="" selected="selected">Seleccione</option>
                                                </select>
                                            </div>
                                            <div ng-show="altaInfraccion.DGSector.$error.required && altaInfraccion.DGSector.$dirty" ng-style="{color:'red'}">El sector es requerido</div>
                                        </div>
                                        <!-- /.form-group -->
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label>Unidad Territorial:</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i
													class="fa fa-random"></i></span>
                                                <select name = "UnidadTer" class="form-control" ng-disabled="!DG.Sector" ng-options="unidadT as unidadT.utNombre for unidadT in DG.unidadesTerritoriales" ng-model="DG.UnidadT">
                                                    <option value="" selected="selected">Seleccione</option>
                                                </select>
                                            </div>
                                            <!-- /.form-group -->
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group" ng-class="{'has-error': (altaInfraccion.DGResponsableVehiculo.$invalid && altaInfraccion.DGResponsableVehiculo.$dirty) }">
                                            <label> *Responsable del Vehículo:</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-user"></i></span>
                                                <select class="form-control" id="DGResponsableVehiculo" ng-options="catRespVeh as catRespVeh.rVehNombre for catRespVeh in DG.CatalogoresponsableVehiculo | orderBy:'rVehNombre'" ng-model="DG.ResponsableVehiculo" name="DGResponsableVehiculo" ng-required="true">
                                                    <option value="" selected="selected">Seleccione</option>
                                                </select>
                                            </div>
                                            <div ng-show="altaInfraccion.DGResponsableVehiculo.$error.required && altaInfraccion.DGResponsableVehiculo.$dirty" ng-style="{color:'red'}">El responsable del vehículo es requerido</div>
                                        </div>
                                        <!-- /.form-group -->
                                    </div>
                                </div>
                                <h4>Arrastre</h4>
                                <hr />
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <label>Con Arrastre:</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i
													class="fa fa-arrows"></i></span>
                                                <select name="tArrastre" class="form-control " ng-options="catTipArr as catTipArr.descripcion for catTipArr in DG.CatalogoTipoArrastre" ng-model="DG.tipoArrastre">
                                                    <option value="" selected="selected">Seleccione</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group" ng-class="{'has-error': (altaInfraccion.DGTipoUnidad.$invalid && altaInfraccion.DGTipoUnidad.$dirty) }">
                                            <label>*Tipo de Unidad:</label>
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="fa fa-car"></i></span>
                                                <select class="form-control" id="DGTipoUnidad" ng-options="catTipUni as catTipUni.descripcion for catTipUni in DG.CatalogoTipoUnidad" ng-model="DG.tipoUnidad" ng-change="actualizarBuscarCodigo()" name="DGTipoUnidad" ng-required="true">
                                                    <option value="" selected="selected">Seleccione</option>
                                                </select>
                                            </div>
                                            <div ng-show="altaInfraccion.DGTipoUnidad.$error.required && altaInfraccion.DGTipoUnidad.$dirty" ng-style="{color:'red'}">El tipo de unidad es requerido
                                            </div>
                                        </div>
                                        <!-- /.form-group -->
                                    </div>
                                    <!-- /.col -->
                                </div>
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group" ng-class="{'has-error': (altaInfraccion.DGEmpleado.$invalid && altaInfraccion.DGEmpleado.$dirty) || (DG.empleado.placaColor=='red') }">
                                            <label>*Placa Oficial:</label>
                                            <div class="input-group">
                                                <input type="text" class="form-control" ng-model="DG.empleado.empPlaca" ng-change="typePlacaDeEmpleado()" ng-required="true" name="DGEmpleado"> <span class="input-group-btn"><button
														type="button" class="btn btn-danger btn-flat"
														ng-click="buscarPlacaPorCodigo()">Buscar</button> </span>
                                                <!--para input placa oficial style="color: {{DG.empleado.placaColor}} " buscarPlacaPorCodigo()-->
                                            </div>
                                            <div ng-show="altaInfraccion.DGEmpleado.$error.required && altaInfraccion.DGEmpleado.$dirty" ng-style="{color:'red'}">La placa oficial es requerida</div>
                                            <div ng-show="DG.empleado.placaColor=='red'" ng-style="{color:'red'}">Se requiere de una placa válida
                                            </div>
                                            <div ng-show="DG.empleado.placaColor=='green'">La placa oficial es válida</div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6" ng-show="showBuscaCodigo">
                                        <div class="form-group" ng-class="{'has-error': (altaInfraccion.DGGrua.$invalid && altaInfraccion.DGGrua.$dirty) || (DG.grua.codigoColor=='red') }">
                                            <label>Código:</label>
                                            <div class="input-group">
                                                <input type="text" class="form-control " capitalize capitalize ng-disabled="DG.disabledBuscaCodigo" ng-model="DG.grua.gruaCod" ng-change="typeCodigoDeGrua()" ng-required="true" name="DGGrua">
                                                <!--para input codigo style="color: {{DG.grua.codigoColor}}" -->
                                                <span class="input-group-btn">
													<button type="button" class="btn btn-danger btn-flat"
														ng-disabled="DG.disabledBuscaCodigo"
														ng-click="buscarGruaPorCodigo()">Buscar</button>
												</span>
                                            </div>
                                            <div ng-show="altaInfraccion.DGGrua.$error.required && altaInfraccion.DGGrua.$dirty" ng-style="{color:'red'}">El código es requerido</div>
                                            <div ng-show="DG.grua.codigoColor=='red'" ng-style="{color:'red'}">Se requiere de un código válido
                                            </div>
                                            <div ng-show="DG.grua.codigoColor=='green'">El código es válido</div>
                                        </div>
                                    </div>
                                </div>
                                <div class="table-responsive">
                                    <fieldset class="scheduler-border" ng-show="DG.empleado.valido">
                                        <legend class="scheduler-border">Datos del Oficial</legend>
                                        <table class="table tblsnborder">
                                            <tr>
                                                <td><dt>Nombre:</dt></td>
                                                <td>{{DG.empleado.empApePaterno}} {{DG.empleado.empApeMaterno}} {{DG.empleado.empNombre}}</td>
                                                <td><dt>Placa:</dt></td>
                                                <td>{{DG.empleado.empPlaca}}</td>
                                            </tr>
                                            <tr>
                                                <td><dt>Agrupamiento:</dt></td>
                                                <td>{{DG.empleado.agrupamiento.agrupacionNombre}}</td>
                                                <td><dt>Sector:</dt></td>
                                                <td>{{DG.empleado.sector.secNombre}}</td>
                                            </tr>
                                        </table>
                                    </fieldset>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="box box-danger">
                        <div class="box-header with-border text-center">
                            <h3 class="box-title">Datos del Vehículo</h3>
                        </div>
                        <!-- /.box-header -->
                        <div class="box-body">
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label for="example4">*Cuenta con Placa:</label>
                                        <div class="row">
                                            <div class="col-lg-3">
                                                <div class="radio">
                                                    <label>
                                                        <input type="radio" ng-model="DV.cuentaConPlaca" value="S" ng-change="cambioCuenaConPlaca()"> Si
                                                    </label>
                                                </div>
                                            </div>
                                            <!-- /.col-lg-6 -->
                                            <div class="col-lg-3">
                                                <div class="radio">
                                                    <label>
                                                        <input type="radio" ng-model="DV.cuentaConPlaca" value="N" ng-change="cambioCuenaConPlaca()"> No
                                                    </label>
                                                </div>
                                            </div>
                                            <!-- /.col-lg-6 -->
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group" ng-class="{'has-error': (altaInfraccion.DVPlaca.$invalid && altaInfraccion.DVPlaca.$dirty) }">
                                        <label for="DVPlaca">*Placa vehículo:</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i
												class="fa fa-automobile"></i></span>
                                            <input type="text" ng-required="DV.cuentaConPlaca == 'S'" class="form-control " capitalize id="DVPlaca" ng-model="DV.placa" ng-disabled="DV.placaDisabled" name="DVPlaca" maxlength="9" />
                                        </div>
                                        <div ng-show="altaInfraccion.DVPlaca.$error.required && altaInfraccion.DVPlaca.$dirty" ng-style="{color:'red'}">La placa vehículo es requerida</div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label>*Expedida en:</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i class="fa fa-map"></i></span>
                                            <select class="form-control" ng-options="estado as estado.edoNombre for estado in DV.catalogoEstados | orderBy:'edoNombre'" ng-model="DV.estado" ng-change="verificaEstadoExpedicion()">
                                                <option value="" selected="selected">Seleccione</option>
                                            </select>
                                        </div>
                                        <!-- /.form-group -->
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group" ng-class="{'has-error': (altaInfraccion.DVTarjetaCirculacion.$invalid && altaInfraccion.DVTarjetaCirculacion.$dirty) }">
                                        <label for="tarjetaCirculacion">*Tarjeta de Circulación: </label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i
												class="fa  fa-credit-card"></i></span>
                                            <input type="text" class="form-control" capitalize capitalize id="DVTarjetaCirculacion" ng-model="DV.tarjetaCirculacion" ng-required="true" name="DVTarjetaCirculacion" maxlength="45">
                                        </div>
                                        <div ng-show="altaInfraccion.DVTarjetaCirculacion.$error.required && altaInfraccion.DVTarjetaCirculacion.$dirty" ng-style="{color:'red'}">La tarjeta de circulación es requerida
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6" ng-if="DV.showSelectGarantiaCatDoc">
                                    <div class="form-group" ng-class="{'has-error': altaInfraccion.DVgarantiaDocumento.$invalid && altaInfraccion.DVgarantiaDocumento.$dirty }">
                                        <label for="tarjetaCirculacion">*Garantía Documento: </label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i
												class="fa  fa-credit-card"></i></span>
                                            <select class="form-control" name="DVgarantiaDocumento" id="DVgarantiaDocumento" ng-options="gDoc as gDoc.garantiaCatNombre for gDoc in DV.catalogoGarantiasDocumentos | orderBy:'garantiaCatNombre'" ng-model="DV.garantiaDocumento" ng-required="DV.showSelectGarantiaCatDoc">
                                                <option value="" selected="selected">Seleccione</option>
                                            </select>
                                        </div>
                                        <div ng-show="DV.showSelectGarantiaCatDoc && altaInfraccion.DVgarantiaDocumento.$invalid && altaInfraccion.DVgarantiaDocumento.$dirty" ng-style="{color:'red'}">El folio de garantía es requerido
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-6" ng-if="DV.showSelectGarantiaCatDoc">
                                    <div class="form-group" ng-class="{'has-error': altaInfraccion.DVGarantiaFolio.$invalid && altaInfraccion.DVGarantiaFolio.$dirty }">
                                        <label for="DVGarantiaFolio">*Folio Garantía:</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i
												class="fa fa-automobile"></i></span>
                                            <input type="text" class="form-control " capitalize capitalize id="DVGarantiaFolio" ng-model="DV.garantiaFolio" ng-required="DV.showSelectGarantiaCatDoc" name="DVGarantiaFolio" />
                                        </div>
                                        <div ng-show="DV.showSelectGarantiaCatDoc && altaInfraccion.DVGarantiaFolio.$invalid && altaInfraccion.DVGarantiaFolio.$dirty" ng-style="{color:'red'}">El folio de garantía es requerido
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <h4>Tipo de Vehículo</h4>
                            <hr />

                            <div class="row">

                                <div class="col-sm-6">
                                    <div class="form-group" ng-class="{'has-error': altaInfraccion.DVTipoVehiculo.$invalid && altaInfraccion.DVTipoVehiculo.$dirty }">
                                        <label>*Tipo:</label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i
												class="fa fa-ellipsis-v "></i></span>
                                            <select class="form-control" ng-options="tipoV as tipoV.vTipoNombre for tipoV in DV.catalogoTiposVehiculo | orderBy:'vTipoNombre'" ng-model="DV.tipoVehiculo" ng-required="true" name="DVTipoVehiculo">
                                                <option value="" selected="selected">Seleccione</option>
                                            </select>
                                        </div>

                                        <div ng-show="altaInfraccion.DVTipoVehiculo.$error.required && altaInfraccion.DVTipoVehiculo.$dirty" ng-style="{color:'red'}">El tipo de vehículo es requerido
                                        </div>
                                        <!-- /.form-group -->
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group">
                                        <label for="example4">Origen: </label>
                                        <div class="row">
                                            <div class="col-lg-6">
                                                <div class="radio">
                                                    <label>
                                                        <input type="radio" ng-model="DV.origen" value="N"> Nacional
                                                    </label>
                                                </div>
                                            </div>
                                            <!-- /.col-lg-6 -->
                                            <div class="col-lg-6">
                                                <div class="radio">
                                                    <label>
                                                        <input type="radio" ng-model="DV.origen" value="E"> Extranjero
                                                    </label>
                                                </div>
                                            </div>
                                            <!-- /.col-lg-6 -->
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-sm-6">
                                    <label>Marca: </label>
                                    <div class="input-group">
                                        <span class="input-group-addon"><i
											class="fa fa-registered"></i></span>
                                        <select name="vMarca" class="form-control" ng-options="marca as marca.vMarNombre for marca in DV.catalogoVehiculosMarcas" ng-model="DV.vehiculoMarca" ng-change="actualizarVehiculosModeloPorMarca()">
                                            <option value="" selected="selected">Seleccione</option>
                                        </select>
                                    </div>
                                    <!-- /.form-group -->
                                </div>

                                <div class="col-sm-6">
                                    <label>Modelo: </label>
                                    <div class="input-group">
                                        <span class="input-group-addon"><i
											class="fa fa-barcode"></i></span>
                                        <select name="vModelo" class="form-control" ng-disabled="!DV.vehiculoMarca" ng-options="modelo as modelo.vModNombre for modelo in DV.catalogoVehiculosModelo" ng-model="DV.vehiculoModelo">
                                            <option value="">Seleccione</option>
                                        </select>
                                    </div>
                                    <!-- /.form-group -->
                                </div>
                            </div>
                            </br>

                            <div class="row">
                                <div class="col-sm-6">
                                    <label>Color: </label>
                                    <div class="input-group">
                                        <span class="input-group-addon"><i
											class="fa fa-paint-brush"></i></span>
                                        <select name = "vColor" class="form-control" ng-options="color as color.vColorNombre for color in DV.catalogoVehiculosColor" ng-model="DV.vehiculoColor">
                                            <option value="">Seleccione</option>
                                        </select>
                                    </div>
                                    <!-- /.form-group -->
                                </div>

                                <div class="col-sm-6">
                                    <label for="serviciopublico" style="cursor: pointer;">Servicio Público: </label>
                                    <div class="row">
                                        <div class="col-lg-7"></div>
                                        <div class="col-lg-2">
                                            <input type="checkbox" id="serviciopublico" name="sectorPublico" ng-true-value="'S'" ng-false-value="'N'" ng-model="DV.sectorPublico">
                                        </div>
                                        <div class="col-lg-3"></div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </section>
                <section class="col-lg-6 connectedSortable">
                    <div class="box box-danger">
                        <div class="box-header with-border text-center">
                            <h3 class="box-title">Ubicación del Automóvil</h3>
                        </div>
                        <div class="box-body">
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="form-group" ng-class="{'has-error': (altaInfraccion.UBICalle.$invalid && altaInfraccion.UBICalle.$dirty) }">
                                        <label for="calle">*En la Calle: </label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i
												class="fa fa-long-arrow-up"></i></span>
                                            <input type="text" class="form-control " capitalize id="calle" ng-model="UBI.calle" name="UBICalle" ng-required="true" maxlength="45">
                                        </div>
                                        <div ng-show="altaInfraccion.UBICalle.$error.required && altaInfraccion.UBICalle.$dirty" ng-style="{color:'red'}">La calle es requerida</div>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group" ng-class="{'has-error': (altaInfraccion.UBIEntreCalle1.$invalid && altaInfraccion.UBIEntreCalle1.$dirty) }">
                                        <label for="entreCalle1">*Entre la Calle: </label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i
												class="fa  fa-arrows-v"></i></span>
                                            <input type="text" class="form-control " capitalize capitalize id="entreCalle1" ng-model="UBI.entreCalle1" ng-required="true" name="UBIEntreCalle1" maxlength="45">
                                        </div>
                                        <div ng-show="altaInfraccion.UBIEntreCalle1.$error.required && altaInfraccion.UBIEntreCalle1.$dirty" ng-style="{color:'red'}">Entre la calle es requerido</div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="form-group" ng-class="{'has-error': (altaInfraccion.UBIEntreCalle2.$invalid && altaInfraccion.UBIEntreCalle2.$dirty) }">
                                        <label for="entreCalle2">*Y en la calle: </label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i
												class="fa fa-long-arrow-down"></i></span>
                                            <input type="text" class="form-control " capitalize capitalize id="entreCalle2" ng-model="UBI.entreCalle2" ng-required="true" name="UBIEntreCalle2" maxlength="45">
                                        </div>
                                        <div ng-show="altaInfraccion.UBIEntreCalle2.$error.required && altaInfraccion.UBIEntreCalle2.$dirty" ng-style="{color:'red'}">Y en la calle es requerido</div>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group" ng-class="{'has-error': (altaInfraccion.UBIDireccion.$invalid && altaInfraccion.UBIDireccion.$dirty) }">
                                        <label for="example1">*Con Dirección: </label>
                                        <div class="input-group">
                                            <span class="input-group-addon"><i
