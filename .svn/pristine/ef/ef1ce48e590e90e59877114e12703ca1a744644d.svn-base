 .list li ul { 
     display: none; 
 }
 
.list {
	list-style: none;
  	padding: 0;
}

.list li ul{
	list-style: none;
  	padding: 0;
  	margin-left: 1em;
}

.list li .plus:before {
	content: "\f067"; /* FontAwesome Unicode */
	font-family: FontAwesome;
	display: inline-block;
	margin-left: -1.3rem; /* same as padding-left set on li */
	width: 1.3rem; /* same as padding-left set on li */
}

.list li .minus:before {
	content: "\f068"; /* FontAwesome Unicode */
	font-family: FontAwesome;
	display: inline-block;
	margin-left: -1.3rem; /* same as padding-left set on li */
	width: 1.3rem; /* same as padding-left set on li */
}