<html>
<head>
<meta charset="utf-8">
<title>Modificación de Infracción</title>
</head>
<body>
	<section class="content-header">
		<h1>Modificación de Infracción</h1>
		<ol class="breadcrumb">
			<li><a><i class="fa fa-book"></i>Infracción</a></li>
			<li class="active">Modificación</li>
		</ol>
	</section>

	<!-- Main content -->
	<section class="content">
		<form id="form1" role="form" name="modificaInfraccion" novalidate>
			<div class="row">
				<section class="col-lg-6 connectedSortable">

					<!-- Datos generales -->
					<div class="box box-danger">
						<div class="box-header with-border text-center">
							<h3 class="box-title">Datos Generales</h3>
						</div>
						<div class="box-body">
							<div class="form-group">
								<div class="row">
									<div class="col-sm-6">
										<div class="form-group">
											<label for="DGNCI">*NCI: </label>
											<div class="input-group date ">
												<div class="input-group-addon">
													<i class="fa fa-building-o"></i>
												</div>
												<input type="text" class="form-control " capitalize id="DGNCI" ng-model="DG.nci" disabled
													ng-required="true" name="DGNCI">
											</div>
										</div>
									</div>
									<div class="col-sm-6">
										<div class="form-group">
											<label for="DGPagada">*Estatus Pago: </label>
											<div class="input-group date ">
												<div class="input-group-addon">
													<i class="fa fa-building-o"></i>
												</div>
												<input type="text" class="form-control " capitalize id="DGPagada"  ng-model="DG.pagada" disabled
													ng-required="true" name="DGPagada">
											</div>
										</div>
									</div>
								</div>
								
								<div class="row">
									<div class="col-sm-6">
										<div class="form-group">
											<label for="DGInfraccion">*Infracción: </label>
											<div class="input-group date ">
												<div class="input-group-addon">
													<i class="fa fa-calendar"></i>
												</div>
												<input type="text" class="form-control " capitalize id="DGInfraccion" disabled
													data-date-format="dd/mm/yyyy" ng-model="DG.infraccion"
													ng-required="true" name="DGInfraccion">
											</div>
										</div>
									</div>
									
									<div class="col-sm-6">
										<div class="form-group">
											<label for="example2">Infracción Impresa:</label>

											<div class="input-group">
												<span class="input-group-addon"><i
													class="fa fa-print"></i></span> <input
													type="text" class="form-control" id="example2" 
													ng-model="DG.infrac_impresa" name="DGInfracEmpresa" maxlength="17">
											</div>
										</div>

									</div>
								</div>
								
								<div class="row">
									<div class="col-sm-6">
										<div class="form-group"
											ng-class="{'has-error': modificaInfraccion.DGFecha.$invalid && modificaInfraccion.DGFecha.$dirty}">
											<label for="example1">*Fecha Infracción:</label>
											
											<div class="input-group date " datetimepicker options="dateTimePickerOptions"
													ng-required="true"
													ng-model="DG.fecha"
													name="DGFecha"
													on-hide="updateFIFecha()" show-button-bar="true">
													
												<div class="input-group-addon">
													<i class="fa fa-calendar"></i>
												</div>
												<input type="text" class="form-control inputFecha input-append"
													 readonly>
											</div>
									
											<div
												ng-show="modificaInfraccion.DGFecha.$error.required && modificaInfraccion.DGFecha.$dirty"
												ng-style="{color:'red'}">La fecha infracción es requerida</div>
										</div>
									</div>
									<div class="col-sm-6">
										<div class="form-group" >
											<label for="DGInfraccionFecha">*Fecha de Emisión: </label>
											<div class="input-group date ">
												<div class="input-group-addon">
													<i class="fa fa-calendar"></i>
												</div>
												<input type="text" class="form-control" id="DGInfraccionFecha" disabled
													data-date-format="dd/mm/yyyy" ng-model="DG.infraccionFecha"
													name="DGInfraccionFecha"
													readonly
													date-picker	idioma-dp="{{currentLanguage}}" />

											</div>
											<div
												ng-show="modificaInfraccion.DGFecha.$error.required && modificaInfraccion.DGFecha.$dirty"
												ng-style="{color:'red'}">La fecha emisión es requerida</div>
										</div>
									</div>
									
								</div>

								<div class="row">
								
									<div class="col-sm-6">
									
										<div class="form-group">
										<!--  <span class="badge" style="background-color:transparent; width:50%; padding:0px;"><img src="static/dist/img/loading.gif"  class="ajax-loader img-responsive center-block" style="width:25%; border:0px; margin:0px;"/></span> -->
										<label >*Estado:</label><br>
											
											
											<div class="input-group">
												
												<span class="input-group-addon"><i
													class="fa fa-map-marker"></i></span> <select class="form-control"
													disabled="true" ng-model="DG.estado">
													<option value="{{DF}}" selected="selected">{{DF.edoNombre}}</option>
												</select>
												<span class="input-group-addon">
													<img src="static/dist/img/loading.gif"  class="ajax-loader img-responsive center-block" style="width:10%"/>
												</span>

											</div>
										</div>
										<!-- /.form-group -->
									</div>
									<div class="col-sm-6">
										<div class="form-group"
											ng-class="{'has-error': modificaInfraccion.DGSector.$invalid && modificaInfraccion.DGSector.$dirty}">
											<label>*Sector: </label>
											<div class="input-group">
												<span class="input-group-addon"><i
													class="fa fa-pie-chart"></i></span> <select class="form-control"
													name="DGSector"
													ng-options="sector.secId as sector.secCod + '&nbsp;&nbsp;&nbsp;' + sector.secNombre for sector in DG.sectores"
													ng-model="DG.Sector"
													ng-change="actualizarUnidadTerritorialPorSector()"
													ng-required="true">
													<option value="" selected="selected">Seleccione</option>
												</select>
											</div>
											<div
												ng-show="modificaInfraccion.DGSector.$error.required && modificaInfraccion.DGSector.$dirty"
												ng-style="{color:'red'}">El sector es requerido</div>
										</div>
										<!-- /.form-group -->
									</div>

								</div>

								<div class="row">

									<div class="col-sm-6">
										<div class="form-group">
											<label>Unidad Territorial :</label>
											<div class="input-group">
												<span class="input-group-addon"><i
													class="fa fa-random"></i></span> <select class="form-control"
													ng-options="unidadT.utId.utId as unidadT.utNombre for unidadT in DG.unidadesTerritoriales"
													ng-model="DG.UnidadT">
													<option value="" selected="selected">Seleccione</option>
												</select>
											</div>
											<!-- /.form-group -->
										</div>
									</div>
									<div class="col-sm-6">
										<div class="form-group"
											ng-class="{'has-error': (modificaInfraccion.DGResponsableVehiculo.$invalid && modificaInfraccion.DGResponsableVehiculo.$dirty) }">
											<label> *Responsable del Vehículo:</label>
											<div class="input-group">
												<span class="input-group-addon"><i class="fa fa-user"></i></span>
												<select class="form-control" id="DGResponsableVehiculo"
													ng-options="catRespVeh.rVehId as catRespVeh.rVehNombre for catRespVeh in DG.CatalogoresponsableVehiculo"
													ng-model="DG.ResponsableVehiculo" ng-required="true"
													name="DGResponsableVehiculo">
													<option value="" selected="selected">Seleccione</option>
												</select>
											</div>
											<div
												ng-show="modificaInfraccion.DGResponsableVehiculo.$error.required && modificaInfraccion.DGResponsableVehiculo.$dirty"
												ng-style="{color:'red'}">El responsable del vehículo
												es requerido</div>
										</div>
										<!-- /.form-group -->
									</div>
								</div>
								<h4>Arrastre</h4>
								<hr />
								<div class="row">
									<div class="col-sm-6">
										<div class="form-group">
											<label>Con Arrastre:</label>
											<div class="input-group">
												<span class="input-group-addon"><i
													class="fa fa-arrows"></i></span> <select class="form-control "
													ng-options="catTipArr.codigoString as catTipArr.descripcion for catTipArr in DG.CatalogoTipoArrastre"
													ng-model="DG.tipoArrastre"
													disabled="true">
<!-- 													<option value="" selected="selected">Seleccione</option> -->
												</select>
											</div>
										</div>
									</div>
									<div class="col-sm-6">
										<div class="form-group"
											ng-class="{'has-error': (modificaInfraccion.DGTipoUnidad.$invalid && modificaInfraccion.DGTipoUnidad.$dirty) }">
											<label>Tipo de Unidad:</label>
											<div class="input-group">
												<span class="input-group-addon"><i class="fa fa-car"></i></span>
												<select class="form-control" id="DGTipoUnidad"
													ng-options="catTipUni.codigoString as catTipUni.descripcion for catTipUni in DG.CatalogoTipoUnidad"
													ng-model="DG.tipoUnidad"
													ng-change="actualizarBuscarCodigo()" ng-required="true"
													name="DGTipoUnidad">
													<option value="" selected="selected">Seleccione</option>
												</select>
											</div>
											<div
												ng-show="modificaInfraccion.DGTipoUnidad.$error.required && modificaInfraccion.DGTipoUnidad.$dirty"
												ng-style="{color:'red'}">El tipo de unidad es
												requerido</div>
										</div>
										<!-- /.form-group -->
									</div>
									<!-- /.col -->
								</div>

								<div class="row">
									<div class="col-sm-6">
										<div class="form-group"
											ng-class="{'has-error': (modificaInfraccion.DGEmpleado.$invalid && modificaInfraccion.DGEmpleado.$dirty) || (DG.empleado.placaColor=='red') }">
											<label> Placa Oficial:</label>
											<div class="input-group">
												<input type="text" class="form-control " capitalize
													ng-model="DG.empleado.empPlaca"
													ng-change="typePlacaDeEmpleado()"
													ng-required="true"
													name="DGEmpleado"> <span class="input-group-btn"><button
														type="button" class="btn btn-danger btn-flat"
														ng-click="buscarPlacaPorCodigo()">Buscar</button> </span>
													<!--para input placa oficial style="color: {{DG.empleado.placaColor}} "-->
											</div>
											<div ng-show="modificaInfraccion.DGEmpleado.$error.required && modificaInfraccion.DGEmpleado.$dirty"
												ng-style="{color:'red'}">La placa oficial es requerida</div>
											<div ng-show="DG.empleado.placaColor=='red'"
												ng-style="{color:'red'}">Se requiere de una placa valida</div>
											<div ng-show="DG.empleado.placaColor=='green'">La placa oficial es valida</div>
										</div>
									</div>
									<div class="col-sm-6" ng-show="showBuscaCodigo">
										<div class="form-group"
											ng-class="{'has-error': (modificaInfraccion.DGGrua.$invalid && modificaInfraccion.DGGrua.$dirty) || (DG.grua.codigoColor=='red') }">
											<label>Código:</label>
											<div class="input-group">
												<input type="text" class="form-control " capitalize
													ng-disabled="disabledBuscaCodigo"
													ng-model="DG.grua.gruaCod" ng-change="typeCodigoDeGrua()"
													ng-required="false"
													name="DGGrua">
													<!--para input codigo style="color: {{DG.grua.codigoColor}}" -->
													 <span class="input-group-btn">
													<button type="button" class="btn btn-danger btn-flat"
														ng-disabled="disabledBuscaCodigo"
														ng-click="buscarGruaPorCodigo()">Buscar</button> </a>
												</span>
											</div>
											<div
												ng-show="modificaInfraccion.DGGrua.$error.required && modificaInfraccion.DGGrua.$dirty"
												ng-style="{color:'red'}">El código es requerido</div>
											<div ng-show="DG.grua.codigoColor=='red'"
												ng-style="{color:'red'}">Se requiere de un código
												valido</div>
											<div ng-show="DG.grua.codigoColor=='green'">El código es valido</div>
										</div>
									</div>
								</div>
								<!-- <div class="row" ng-show="DG.empleado.valido">
									<div class="col-xs-8">
										<div class="form-group">
											<label>Nombre:</label>
											<div class="input-group" style="width: 100%">
												<input type="text" class="form-control" disabled="true"
													value="{{DG.empleado.empApePaterno}} {{DG.empleado.empApeMaterno}} {{DG.empleado.empNombre}}"
													style="color: green;">
											</div>
										</div>
									</div>
									<div class="col-xs-4">
										<label>Id:</label>
										<div class="input-group">
											<input type="text" class="form-control" disabled="true"
												value="{{DG.empleado.empId}}"
												style="color: green">
										</div>
									</div>
								</div>
								<div class="row" ng-show="DG.empleado.valido">
									<div class="col-sm-6">
										<div class="form-group">
											<label>Agrupamiento:</label>
											<div class="input-group" style="width: 100%">
												<input type="text" class="form-control" disabled="true"
													value="{{DG.empleado.agrupamiento.agrupacionNombre}}"
													style="color: green;">
											</div>
										</div>
									</div>
									<div class="col-sm-6">
										<label>Sector:</label>
										<div class="input-group" style="width: 100%">
											<input type="text" class="form-control" disabled="true"
												value="{{DG.empleado.sector.secNombre}}"
												style="color: green">
										</div>
									</div>
								</div> -->
								<fieldset class="scheduler-border" ng-show="DG.empleado.valido">
									<legend class="scheduler-border">Datos del oficial</legend>
										<div class="table-responsive">
											<table class="table tblsnborder">
												<tr>
                                           		<td><dt>Nombre:</dt></td>
                                           		<td>{{DG.empleado.empApePaterno}} {{DG.empleado.empApeMaterno}} {{DG.empleado.empNombre}}</td>
                                           		<td><dt>Placa:</dt></td>
                                          			<td>{{DG.empleado.empPlaca}}</td>
                                          		</tr>
                                          		<tr>
                                          			<td><dt>Agrupamiento:</dt></td>
                                          			<td>{{DG.empleado.agrupamiento.agrupacionNombre}}</td>
                                          			<td><dt>Sector:</dt></td>
                                          			<td>{{DG.empleado.sector.secNombre}}</td>
                                          		</tr>
										</table>
									</div>
								</fieldset>
							</div>
						</div>
					</div>
						<!-- /.box -->


						<!-- Datos Vehiculo-->
						<div class="box box-danger">
							<div class="box-header with-border  text-center">
								<h3 class="box-title">Datos del Vehículo</h3>
							</div>
							<!-- /.box-header -->
							<div class="box-body">
								<div class="row">
									<div class="col-sm-6">
										<div class="form-group">
											<label for="example4">*Cuenta con Placa:</label>
											<div class="row">
												<div class="col-lg-3">
													<div class="radio">
														<label> <input type="radio"
															ng-model="DV.cuentaConPlaca" value="S"
															ng-change="cambioCuenaConPlaca()"> Si
														</label>
													</div>
												</div>
												<!-- /.col-lg-6 -->
												<div class="col-lg-3">
													<div class="radio">
														<label> <input type="radio"
															ng-model="DV.cuentaConPlaca" value="N"
															ng-change="cambioCuenaConPlaca()"> No
														</label>
													</div>
												</div>
												<!-- /.col-lg-6 -->
											</div>
										</div>
									</div>
									<div class="col-sm-6">
										<div class="form-group"
											ng-class="{'has-error': (modificaInfraccion.DVPlaca.$invalid && modificaInfraccion.DVPlaca.$dirty) }">
											<label for="DVPlaca">*Placa Vehículo:</label>
											<div class="input-group">
												<span class="input-group-addon"><i
													class="fa fa-automobile"></i></span> <input type="text"
													class="form-control " capitalize id="DVPlaca" ng-model="DV.placa"
													ng-disabled="DV.placaDisabled" ng-required="DV.cuentaConPlaca == 'S'"
													name="DVPlaca" maxlength="9"/>
											</div>
											<div
												ng-show="modificaInfraccion.DVPlaca.$error.required && modificaInfraccion.DVPlaca.$dirty"
												ng-style="{color:'red'}">La placa vehículo es requerida</div>
										</div>
									</div>
								</div>
								<div class="row">

									<div class="col-sm-6">
										<div class="form-group">
											<label>*Expedida en:</label>
											<div class="input-group">
												<span class="input-group-addon"><i class="fa fa-map"></i></span>
												<select class="form-control"
													ng-options="estado.edoId as estado.edoNombre for estado in DV.catalogoEstados"
													ng-model="DV.estado" ng-change="verificaEstadoExpedicion()">
												</select>
											</div>
											<!-- /.form-group -->
										</div>
									</div>
									<div class="col-sm-6">
										<div class="form-group"
											ng-class="{'has-error': (modificaInfraccion.DVTarjetaCirculacion.$invalid && modificaInfraccion.DVTarjetaCirculacion.$dirty) }">
											<label for="tarjetaCirculacion">*Tarjeta de
												Circulación: </label>
											<div class="input-group">
												<span class="input-group-addon"><i
													class="fa  fa-credit-card"></i></span> <input type="text"
													class="form-control " capitalize id="DVTarjetaCirculacion"
													ng-model="DV.tarjetaCirculacion" ng-required="true"
													name="DVTarjetaCirculacion" maxlength="25">
											</div>
											<div
												ng-show="modificaInfraccion.DVTarjetaCirculacion.$error.required && modificaInfraccion.DVTarjetaCirculacion.$dirty"
												ng-style="{color:'red'}">La tarjeta de circulación es
												requerida</div>
										</div>
									</div>
									</div>
									<div class="row">
									<div class="col-sm-6" ng-show="DV.showSelectGarantiaCatDoc">
										<div class="form-group">
											<label for="tarjetaCirculacion">*Garantía Documento:
											</label>
											<div class="input-group">
												<span class="input-group-addon"><i
													class="fa  fa-credit-card"></i></span> <select
													class="form-control"
													ng-options="gDoc.garantiaCatDocumentoId as gDoc.garantiaCatNombre for gDoc in DV.catalogoGarantiasDocumentos"
													ng-model="DV.garantiaDocumento"
													ng-required="DV.showSelectGarantiaCatDoc"
													ng-disabled="DV.isGarantiaBlocked">
													<option value="">Seleccione</option>
												</select>
											</div>
										</div>
									</div>
									<div class="col-sm-6" ng-show="DV.showSelectGarantiaCatDoc">
										<div class="form-group"
											ng-class="{'has-error': (DV.garantiaDocumento != null && modificaInfraccion.DVGarantiaFolio.$invalid && modificaInfraccion.DVGarantiaFolio.$dirty && !DV.isGarantiaBlocked) }">
											<label for="DVGarantiaFolio">*Folio Garantía:</label>
											<div class="input-group">
												<span class="input-group-addon"><i
													class="fa fa-automobile"></i></span>
													<input type="text" class="form-control " capitalize 
													id="DVGarantiaFolio" 
													ng-model="DV.garantiaFolio"
 													ng-required="DV.showSelectGarantiaCatDoc && modificaInfraccion.DVGarantiaFolio.$invalid && modificaInfraccion.DVGarantiaFolio.$dirty && !DV.isGarantiaBlocked"
													ng-disabled="DV.isGarantiaBlocked"
													name="DVGarantiaFolio" />
											</div>
											<div
												ng-show="DV.showSelectGarantiaCatDoc && modificaInfraccion.DVGarantiaFolio.$invalid && modificaInfraccion.DVGarantiaFolio.$dirty && !DV.isGarantiaBlocked"
												ng-style="{color:'red'}">El folio de garantía es requerido</div>
										</div>
									</div>
								</div>

								<h4>Tipo de Vehículo</h4>
								<hr />

								<div class="row">

									<div class="col-sm-6">
										<div class="form-group"
											ng-class="{'has-error': modificaInfraccion.DVTipoVehiculo.$invalid && modificaInfraccion.DVTipoVehiculo.$dirty }">
											<label>Tipo:</label>
											<div class="input-group">
												<span class="input-group-addon"><i
													class="fa fa-ellipsis-v "></i></span> <select class="form-control"
													ng-options="tipoV.vTipoId as tipoV.vTipoNombre for tipoV in DV.catalogoTiposVehiculo"
													ng-model="DV.tipoVehiculo" ng-required="true"
													name="DVTipoVehiculo">
													<option value="" selected="selected">Seleccione</option>
												</select>
											</div>

											<div
												ng-show="modificaInfraccion.DVTipoVehiculo.$error.required && modificaInfraccion.DVTipoVehiculo.$dirty"
												ng-style="{color:'red'}">El tipo de vehiculo es
												requerido</div>
											<!-- /.form-group -->
										</div>
									</div>
									<div class="col-sm-6">
										<div class="form-group">
